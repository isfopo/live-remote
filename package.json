{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check",
    "install": "npm run build && python3 runners/install.py --name LiveRemote",
    "watch": "python3 runners/watch.py --version 'Live 12.0.15'",
    "close-set": "pkill -x Ableton Live 12 Suite",
    "open-set": "open set/set.als",
    "reload": "npm run install && npm run close-set && sleep 1 && npm run open-set"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "typescript": "^4.9.0",
    "tslib": "^2.5.0",
    "@tsconfig/svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
}